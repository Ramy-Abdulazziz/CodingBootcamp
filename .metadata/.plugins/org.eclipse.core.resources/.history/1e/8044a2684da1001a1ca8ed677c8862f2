package variables;

public class PrimitiveVariables {

	public static void main(String[] args) {
		
		//8 primitive variables 
		
		int x; 
		
		System.out.println(Integer.MAX_VALUE + " -- " + Integer.MIN_VALUE);
		
		
		x = 2147483647;
		
		System.out.println(x);
		System.out.println(x+1);
		
		
		
	}

}
